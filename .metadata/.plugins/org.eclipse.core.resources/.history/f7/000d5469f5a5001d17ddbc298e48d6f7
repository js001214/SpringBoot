package com.mysite.sbb;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@Getter
@Setter
@NoArgsConstructor	//기본 생성자 :
@ToString
@AllArgsConstructor	// 객체 생성시 모든 필드의 값을 입력 받아 필드의 초기값을 할당
public class HelloLomBok3 {

	private String hello;	//필드 : private
	private int lombok;		//필드
	/*
	//public HelloLomBok3(){}	<== 기본 생성자 // NoArgsConsturctor
	//public HElloLomBok3(String hello, int lombok){ // AllArgsConsturctor
	 this.hello = hello;
	 this.lombok = lombok;
	*/
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//@AllArgsConstructor 테스트
		HelloLomBok3 lombok3 = new HelloLomBok3(); 

	}

}
